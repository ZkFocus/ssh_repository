<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="address" class="com.hp.demo.autowire.Address" p:city="ZhengZhou" p:province="HeNan"></bean>
<bean id="student" class="com.hp.demo.autowire.Student" p:name="ZhangSan"></bean>
<bean id="teacher" class="com.hp.demo.autowire.Teacher" p:name="Lisi" autowire="byType"></bean>



</beans>
